  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/express-handlebars": "^5.3.1",
    "@types/jest": "^27.0.1",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/jwt-simple": "^0.5.33",
    "@types/pg": "^8.6.1",
